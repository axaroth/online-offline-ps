<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="collective.Book">
      
<metal:block fill-slot="top_slot"
             tal:define="dummy python:request.set('disable_border',1)" />
                
<body>

      
  <div metal:fill-slot="main" id="search-results-page">
    <tal:main-macro metal:define-macro="main">
      <div id="main-content" class="grid_3">
        <h1 class="documentFirstHeading">Search results <span id="search-term"> </span></h1>

        <ul id="search-results">
        </ul>
        
        <script dump="true" type="text/javascript"
      tal:attributes="src string:${portal_url}/oops.search.js"></script>

        
      </div>
    </tal:main-macro>
  </div>
  
</body>
</html>
